var dataPath = '${basePath}';
$(".loading").height($(window).innerHeight());
var current = 0; 
i = 0;
j = 0;
count = 0;
var t2='';
//初始化调用
t1 = setInterval("start1()",10);
var interval = setInterval(increment,60);  
function start1(){
	i = i + 0.6;
	count = count + 1;
	if(count>=300){
		count = 0;
		clearInterval(t1);
		t2 = setInterval("start2()",10);
	};
	$(".pie1").css("-o-transform","rotate(" + i + "deg)");
	$(".pie1").css("-moz-transform","rotate(" + i + "deg)");
	$(".pie1").css("-webkit-transform","rotate(" + i + "deg)");
};
function start2(){
	j = j + 0.6;
	count = count + 1;
	if(count>=250){
		count = 0;
		clearInterval(t2);
	};
	$(".pie2").css("-o-transform","rotate(" + j + "deg)");
	$(".pie2").css("-moz-transform","rotate(" + j + "deg)");
	$(".pie2").css("-webkit-transform","rotate(" + j + "deg)");
};
 
function increment(){  
  current++;  
  $('#counts').html(current+'%');   
  if(current == 90) { current = 90; clearInterval(interval);}  
}  
window.onload=function()
{
	var myAuto = document.getElementById('myaudio');
	myAuto.play();
	
	$(".musicStyle img").toggle(function(){
			$(".musicStyle img").eq(0).hide();
			$(".musicStyle img").eq(1).show();
			myAuto.pause();
		},function(){
			$(".musicStyle img").eq(1).hide();
			$(".musicStyle img").eq(0).show();
			myAuto.play();
		}) ;
		
	clearInterval(t2);
	clearInterval(t1);
	$(".pie1,.pie2").css("-o-transform","rotate(180deg)");
	$(".pie1,.pie2").css("-moz-transform","rotate(180deg)");
	$(".pie1,.pie2").css("-webkit-transform","rotate(180deg)");
	clearInterval(interval);
